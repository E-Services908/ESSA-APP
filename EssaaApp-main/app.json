{
  "expo": {
    "name": "ESA+",
    "slug": "esa-plus-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#191925"
    },
    "packagerOpts": {
      "minify": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.esaplus.esaplusapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#191925"
      },
      "package": ""
    },
    "web": {
      "favicon": "./assets/logo.jpeg"
    }
  }
}
